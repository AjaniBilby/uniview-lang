import "experimental/time.uv" as Time;
import "print.uv";

fn main(): int {
	let now = Time.Now();

	let dateTime = Time.LocalDateTime(now);

	let time = dateTime.time;
	print(time.hours);
	print(":");
	print(time.minutes);
	print(":");
	print(time.seconds);

	print(" ");
	let date = dateTime.date;
	print(date.day);
	print("/");
	print(date.month);
	print("/");
	print(date.year);

	return 0;
}