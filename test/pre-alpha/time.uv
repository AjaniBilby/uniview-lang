import "experimental/time.uv" as Time;
import "print.uv";

fn main(): int {
	let now = Time.NowUTC();

	let time = Time.GetTime(now);
	print(time.hours);
	print(":");
	print(time.minutes);
	print(":");
	print(time.seconds);

	print(" ");
	let date = Time.GetDate(now);
	print(date.day);
	print("/");
	print(date.month);
	print("/");
	print(date.year);

	return 0;
}