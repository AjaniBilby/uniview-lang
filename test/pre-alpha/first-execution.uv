/*
	Description: First executable version of Qupa
	Goals: Compiles to LLVM-IR
	  Able to do integer addition
		Print result
		Exit code 0
*/


external assume {
	fn i32_println(val: i32);
}


fn main (): int {
	let a = 1;
	let b = 2;

	i32_println(a);
	i32_println(b);

	let c = a + b;
	i32_println(c);

	c = b;
	i32_println(c);

	return 0;
}