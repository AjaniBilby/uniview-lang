import "print.uv";

struct Person {
	id: int;
	age: int;
}


fn init(ofAge: bool): Person {
	let p = Blank#[Person]();
	if (ofAge) {
		p.age = 21;
	} else {
		p.age = 16;
	}

	p.id = 0;
	return p;
}

fn age(p: @Person) {
	p.age = p.age + 1;
	return;
}

fn print(p: Person) {
	print("Person { id: ");
	print(p.id);
	print(", age: ");
	print(p.age);
	println(" }");

	return;
}


fn main(): int {
	let p = init(false);
	let b = $p;
	age(@p);
	print(p);
	print(b);

	return 0;
}