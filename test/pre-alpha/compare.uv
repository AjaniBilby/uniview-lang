fn CompareFloats() {
	let a = 3.0;
	let b = 7.0;

	let c = a == b;
	c = a != b;
	c = a > b;
	c = a < b;
	c = a >= b;
	c = a <= b;
	return;
}

fn CompareInts() {
	let a = 3;
	let b = 7;

	let c = a == b;
	c = a != b;
	c = a > b;
	c = a < b;
	c = a >= b;
	c = a <= b;
	return;
}

fn CompareBools () {
	let a = true;
	let b = false;

	let c = !a;
	c = a || b;
	c = a && b;

	return;
}

fn main(): int {
	CompareFloats();
	CompareInts();
	CompareBools();

	return 0;
}